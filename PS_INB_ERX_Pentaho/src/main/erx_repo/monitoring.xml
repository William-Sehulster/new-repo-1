    <connection>local_erx</connection>
    <sql>select 
(select value from app_configuration where key = 'MJ_FROM_ADDRESS') as MJ_FROM_ADDRESS,
(select value from app_configuration where key = 'MJ_SMTP_PORT')  as MJ_SMTP_PORT,
(select value from app_configuration where key = 'MJ_SMTP_SERVER')  as MJ_SMTP_SERVER,
(select value from app_configuration where key = 'MJ_TO_ADDRESS')  as MJ_TO_ADDRESS,
DBMS_XMLGEN.GETXML('select to_char(received_date_time, ''yyyy-mm-dd hh24:mi:ss'') date_time,
number_msgs_rcvd,
number_newrxs,
number_dlvrd_vista,
number_errored
from
(select 
trunc(received_date, ''HH'') received_date_time,
count(inbound_ncpdp_msg_id) number_msgs_rcvd,
sum(decode(message_type, ''NewRx'', 1, 0)) number_newrxs,
sum(decode(message_status, ''3002'', 1, 0)) number_dlvrd_vista,
sum(decode(message_status, ''3005'', 1, 0)) number_errored
from erx.inbound_ncpdp_msg
where received_date > trunc(sysdate - 1)
group by trunc(received_date, ''HH'')
order by trunc(received_date, ''HH''))') xml_results
from dual</sql>
